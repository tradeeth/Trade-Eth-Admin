    "nodemon": "^1.17.4",
    "pg": "^7.4.3",
    "sequelize": "^4.38.0",
    "umzug": "^2.1.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.15.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
